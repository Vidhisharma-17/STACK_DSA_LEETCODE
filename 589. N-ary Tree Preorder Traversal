class Solution {
public:
    void dfs(Node* root,vector<int>& res){
        if(!root) return;
        res.push_back(root->val);
        for(auto child:root->children){
            dfs(child,res);
        }
    }
    vector<int> preorder(Node* root) {
        vector<int> res;
        dfs(root,res);
        return res;
        
    }
};
